{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Databases": {
    "DefaultDb": {
      "ConnectionString": "Server=.;Database=ProductManagementDb;Trusted_Connection=True;TrustServerCertificate=True"
    },
    "ReportingDb": {
      "ConnectionString": "Server=.;Database=ProductReportingDb;Trusted_Connection=True;TrustServerCertificate=True"
    }
  },
  "StorageContainers": {
    "ProductFiles": {
      "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=demoproductsimages;AccountKey=#{AzureStorage:ConnectionString}#=;EndpointSuffix=core.windows.net",
      "ContainerName": "priductimagescontainer",
      "Url": "https://demoproductsimages.blob.core.windows.net/priductimagescontainer/"
    },
    "Reports": {
      "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=demoproductsimages;AccountKey=#{AzureStorage:ConnectionString}#=;EndpointSuffix=core.windows.net",
      "ContainerName": "priductimagescontainer",
      "Url": "https://demoproductsimages.blob.core.windows.net/priductimagescontainer/"
    }
  },
  "RetryPolicy": {
    "RetryCount": 3,
    "RetryDelayMilliseconds": 2000
  }
}